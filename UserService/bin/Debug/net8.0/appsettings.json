{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "__DB_CONNECTION__"
  },
  "Jwt": {
    "Key": "__JWT_KEY__",
    "Issuer": "__JWT_ISSUER__",
    "Audience": "__JWT_AUDIENCE__",
    "ExpiryInMinutes": 5
  },
  "Redis": {
    "Host": "__REDIS_HOST__",
    "Port": "__REDIS_PORT__",
    "User": "__REDIS_USER__",
    "Password": "__REDIS_PASSWORD__",
    "AllowAdmin": "__REDIS_ALLOW_ADMIN__"
  }
}